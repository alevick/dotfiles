function sshls() {
    id=$1
	ip=`/mnt/common/registry/ls_all_ec2.sh | grep -m1 $id | cut -f3`
    echo "sshing to $ip"
	ssh $ip
}

function sshi() {
    id=$1
	ip=`/mnt/common/registry/discover.py | grep -m1 $id | cut -f6`
    echo "sshing to $ip"
	ssh $ip
}

aws0() {
    export AWS_ACCESS_KEY_ID=$AWS_AN
    export AWS_SECRET_ACCESS_KEY=$AWS_AN_KEY
}

aws1() {
    export AWS_ACCESS_KEY_ID=$AWS_PRO
    export AWS_SECRET_ACCESS_KEY=$AWS_PRO_KEY
}
